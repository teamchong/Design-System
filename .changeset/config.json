{
    "$schema": "https://unpkg.com/@changesets/config@2.3.1/schema.json",
    "changelog": [
        "./es-changelog-config.js",
        { "repo": "teamchong/Design-System" }
    ],
    "commit": false,
    "fixed": [],
    "linked": [
        [
            "@eventstore-ui/editor",
            "@eventstore-ui/fields",
            "@eventstore-ui/theme",
            "@eventstore-ui/router",
            "@eventstore-ui/assets",
            "@eventstore-ui/stores",
            "@eventstore-ui/utils",
            "@eventstore-ui/components",
            "@eventstore-ui/forms",
            "@eventstore-ui/illustrations",
            "@eventstore-ui/layout"
        ]
    ],
    "access": "public",
    "baseBranch": "main",
    "updateInternalDependencies": "patch",
    "ignore": [
        "@eventstore-ui/docs",
        "@eventstore-ui/preview-builder",
        "forms-demo",
        "router-demo",
        "stores-demo"
    ],
    "privatePackages": false,
    "___experimentalUnsafeOptions_WILL_CHANGE_IN_PATCH": {
        "onlyUpdatePeerDependentsWhenOutOfRange": true
    }
}
